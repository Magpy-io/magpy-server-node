  "version": "1.0.0",
  "main": "dist/src/main.js",
  "scripts": {
    "start": "yarn start:server",
    "build": "yarn build:server",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "start:server": "yarn build:server && electron-forge start",
    "start:all": "yarn build:all && electron-forge start",
    "build:server": "rimraf ./dist && tsc",
    "build:client": "cd client; yarn build; cd ..",
    "build:all": "yarn build:client && yarn build:server",
    "test": "yarn test:v --silent",
    "test:v": "cross-env NODE_ENV=test jest --verbose=true --collect-coverage --testTimeout=10000",
    "clear:images": "ts-node ./src/clearImages.ts",
    "types:generate": "ts-node ./src/CreateTypes.ts",
    "types:copy": "ts-node ./src/CopyEndpointTypesToExport.ts",
    "clean:types": "rimraf ./src/api/export/Types && rimraf ./src/api/Types/RequestTypes",
    "types": "yarn clean:types && yarn types:generate && yarn types:copy"
  },
  "dependencies": {
    "@sequelize/core": "^7.0.0-alpha.29",
