{
  "name": "opencloud-server",
  "author": "ic",
  "description": "Description",
  "license": "MIT",
  "version": "1.0.0",
  "main": "dist/src/index.js",
  "scripts": {
    "format": "prettier --write ./src ./tests",
    "start": "yarn start:server",
    "build": "yarn build:server",
    "package": "pkg -C GZip -c ./package.json -t node18-win-x64 -o out/app ./dist/src/index.js",
    "convert:exec": "node ./scripts/convert.js out/app.exe out/appConverted.exe",
    "start:server": "yarn build:server && node ./dist/src/index.js",
    "start:all": "yarn build:all && node ./dist/src/index.js",
    "types:check": "tsc --noEmit && echo No typescript errors found",
    "build:server": "rimraf ./dist && tsc",
    "build:client": "cd client; yarn build; cd ..",
    "build:all": "yarn build:client && yarn build:server",
    "test": "yarn test:v --silent",
    "test:v": "cross-env NODE_ENV=test jest --verbose=true --collect-coverage --testTimeout=10000",
    "types:generate": "ts-node ./scripts/CreateTypes.ts",
    "types:copy": "ts-node ./scripts/CopyEndpointTypesToExport.ts",
    "clear:images": "ts-node ./scripts/clearImages.ts",
    "clean:types": "rimraf ./src/api/export/Types && rimraf ./src/api/Types/RequestTypes",
    "clean:backendQueries": "rimraf ./src/modules/BackendQueries",
    "types": "yarn clean:types && yarn types:generate && yarn types:copy"
  },
  "dependencies": {
    "@sequelize/core": "^7.0.0-alpha.29",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mdns": "^0.0.38",
    "@types/mz": "^2.7.8",
    "@types/node": "^20.10.4",
    "@types/sqlite3": "^3.1.11",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@types/validator": "^13.11.7",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "glob": "^10.3.10",
    "ip": "^1.1.8",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mdns": "^2.7.2",
    "mz": "^2.7.0",
    "network": "^0.6.1",
    "pkg": "^5.8.1",
    "rimraf": "^5.0.5",
    "sequelize": "^6.35.2",
    "sharp": "^0.32.6",
    "sqlite3": "^5.1.4",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "typescript": "^5.3.3",
    "typesync": "^0.11.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-typescript": "^7.23.3",
    "@jest/globals": "^29.7.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bcryptjs": "^2",
    "@types/ip": "^1",
    "@types/jest": "^29.5.11",
    "babel-jest": "^29.7.0",
    "create-nodew-exe": "^1.0.10",
    "jest": "^29.7.0",
    "joi-to-typescript": "^4.12.0",
    "memfs": "^4.6.0",
    "prettier": "^3.2.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "typescript-parser": "^2.6.1"
  },
  "packageManager": "yarn@4.0.2",
  "pkg": {
    "scripts": [
      "node_modules/axios/dist/node/axios.cjs"
    ]
  }
}
