{
  "name": "opencloud-server",
  "author": "Magpy",
  "description": "Magpy's Server Project",
  "license": "MIT",
  "version": "0.6.4",
  "main": "dist/src/index.js",
  "scripts": {
    "format": "prettier --write ./src ./tests",
    "start": "yarn start:server",
    "build": "yarn build:server",
    "start:server": "yarn build:server && cross-env NODE_ENV=dev node ./dist/src/index.js",
    "start:all": "yarn build:all && yarn start:server",
    "types:check": "yarn types:check:src && yarn types:check:tests",
    "types:check:src": "tsc --noEmit && echo No typescript errors found in src",
    "types:check:tests": "tsc --project ./tests/tsconfig.json --noEmit && echo No typescript errors found in tests",
    "build:server": "rimraf ./dist && tsc && echo Build server successfull",
    "build:client": "cd client; yarn build; cd .. && echo Build client successfull",
    "build:all": "yarn build:client && yarn build:server",
    "test:base": "yarn test:base:v --silent",
    "test:base:v": "cross-env NODE_ENV=test jest --verbose=true --collect-coverage --testTimeout=100000",
    "test": "yarn test:base ./tests/global ./tests/routes",
    "test:perf": "yarn test:base:v ./tests/performanceTestDB",
    "types:generate": "ts-node ./scripts/CreateTypes.ts",
    "types:copy": "ts-node ./scripts/CopyEndpointTypesToExport.ts",
    "clear:images": "cross-env NODE_ENV=dev ts-node ./scripts/clearImages.ts",
    "clean:types": "rimraf ./src/api/export/Types && rimraf ./src/api/Types/RequestTypes",
    "clean:backendQueries": "rimraf ./src/modules/BackendQueries",
    "types": "yarn clean:types && yarn types:generate && yarn types:copy",
    "bundle": "yarn build:all && rimraf ./bundle && ts-node ./scripts/bundle.ts && yarn bundle:copy",
    "bundle:server": "yarn build:server && rimraf ./bundle && ts-node ./scripts/bundle.ts && yarn bundle:copy",
    "bundle:copy": "ts-node ./scripts/CopyFolder.ts ./bundle ../MagpyServerWindows/MagpyServerWindows/bin/Debug/bundle && ts-node ./scripts/CopyFolder.ts ./bundle ../MagpyServerWindows/MagpyServerWindows/bin/Release/bundle",
    "parseLogs": "ts-node ./scripts/parseLogs.ts"
  },
  "dependencies": {
    "@moebius/http-graceful-shutdown": "^1.1.0",
    "@sequelize/core": "^7.0.0-alpha.29",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mz": "^2.7.8",
    "@types/node": "^20.10.4",
    "@types/sqlite3": "^3.1.11",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@types/validator": "^13.11.7",
    "appdata-path": "https://github.com/coasys/appdata-path.git#commit=6304ea16cbfebc98749c33941d643c3046b45985",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "glob": "^10.3.10",
    "ip": "^1.1.8",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mz": "^2.7.0",
    "network": "^0.6.1",
    "open": "8.4.2",
    "rimraf": "^5.0.5",
    "sequelize": "patch:sequelize@npm%3A6.37.3#~/.yarn/patches/sequelize-npm-6.37.3-e2e584f609.patch",
    "sharp": "^0.32.6",
    "sqlite3": "patch:sqlite3@npm%3A5.1.7#~/.yarn/patches/sqlite3-npm-5.1.7-0efaca1baf.patch",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "typescript": "^5.3.3",
    "typesync": "^0.11.1",
    "umzug": "^3.8.2",
    "uuid": "^9.0.0",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-typescript": "^7.23.3",
    "@jest/globals": "^29.7.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bcryptjs": "^2",
    "@types/cors": "^2",
    "@types/event-stream": "^4",
    "@types/ip": "^1",
    "@types/jest": "^29.5.11",
    "babel-jest": "^29.7.0",
    "esbuild": "^0.23.0",
    "esbuild-plugin-copy": "^2.1.1",
    "event-stream": "^4.0.1",
    "jest": "^29.7.0",
    "joi-to-typescript": "^4.12.0",
    "memfs": "^4.6.0",
    "prettier": "^3.2.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "typescript-parser": "^2.6.1"
  },
  "packageManager": "yarn@4.0.2"
}
