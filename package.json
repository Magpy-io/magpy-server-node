    "test:v": "yarn build:server && cross-env NODE_ENV=test jest ./dist/tests --verbose=true --collect-coverage --testTimeout=10000",
    "test:single": "yarn build:server && cross-env NODE_ENV=test jest --verbose=true --collect-coverage --testTimeout=10000"
  },
