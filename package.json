    "mdns": "^2.7.2",
    "mz": "^2.7.0",
    "rimraf": "^5.0.5",
    "sequelize": "^6.35.2",
    "sharp": "^0.32.6",
    "sqlite3": "^5.1.4",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
